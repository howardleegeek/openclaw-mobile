<?xml version="1.0" encoding="utf-8"?>
<!--
    Network Security Configuration for ClawPhones.
    All domains require HTTPS.
-->
<network-security-config>
    <base-config cleartextTrafficPermitted="false">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>
